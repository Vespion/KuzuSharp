<Project>
	<Target Name="Setup">
		<Exec
				CustomErrorRegularExpression="Error *: *(.*)$"
				YieldDuringToolExecution="true"
				EchoOff="false"
				UseCommandProcessor="false"
				Command="earthly --config $(MSBuildThisFileDirectory)../earthly-config.yml --platform=$(Os)/$(Arch) $(MSBuildThisFileDirectory)..+setup-$(Os)"
		/>
	</Target>
</Project>